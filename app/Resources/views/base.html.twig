<!DOCTYPE html>
<html lang="en"  >
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta http-equiv="Content-Security-Policy" content="default-src 'self' 'unsafe-inline' https://f.vimeocdn.com https://ssl.google-analytics.com https://js-agent.newrelic.com https://bam.nr-data.net https://f.vimeocdn.com https://ajax.googleapis.com https://player.vimeo.com">

    <title>{% block title %} {{ page_title }}{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    {% block stylesheets %}

        {% stylesheets filter='scssphp' output='css/app.css'
        '%kernel.root_dir%/Resources/assets/sass/main.scss'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

</head>
<body>
{% block body %}{% endblock %}

{% block javascripts %}

    {% javascripts filter="?jsqueeze" output="js/app.js"
    '%kernel.root_dir%/../node_modules/jquery/dist/jquery.js'
    '%kernel.root_dir%/../node_modules/bootstrap-sass/assets/javascripts/bootstrap.js'
    '%kernel.root_dir%/Resources/assets/js/*'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

</body>
</html>